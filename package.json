{
  "name": "WebPageTest-Dispatch",
  "version": "0.0.1",
  "description": "Test scheduler for WebPageTest",
  "keywords": [
    "react",
    "react-router",
    "webpack"
  ],
  "main": "index.html",
  "scripts": {
    "start": "NODE_ENV=production PORT=9000 node server/index.js",
    "build": "webpack -p --progress --config webpack-production.config.js",
    "dev": "webpack-dev-server --progress --colors --hot --inline & nodemon server/index.js"
  },
  "author": "@timludikar <tim.ludikar@razorfish.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-jest": "^5.3.0",
    "babel-loader": "^6.1.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "del": "^2.0.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "h2o2": "^5.1.0",
    "jest-cli": "^0.7.1",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "object-assign": "^4.0.1",
    "orchestrator": "^0.3.7",
    "react-addons-test-utils": "^0.14.2",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-register": "^6.7.2",
    "handlebars": "^4.0.5",
    "hapi": "^13.2.1",
    "history": "^1.13.0",
    "immutable": "^3.7.6",
    "inert": "^3.2.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-mdl": "^1.5.2",
    "react-router": "^1.0.3",
    "reflux": "^0.3.0",
    "vision": "^4.0.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/object-assign"
    ]
  }
}
