{
  "name": "webkid-react-starterkit",
  "version": "1.0.1",
  "description": "Lightweight starting point for a react web app.",
  "keywords": [
    "react",
    "starterkit",
    "frontend",
    "boilerplate",
    "reflux",
    "react-router"
  ],
  "main": "index.html",
  "scripts": {
    "start": "NODE_ENV=production PORT=9000 node server.js",
    "build": "webpack -p --progress --config webpack-production.config.js",
    "dev": "webpack-dev-server --progress --colors --hot --inline & nodemon server.js"
  },
  "author": "@moklick <mo@webkid.io>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-jest": "^5.3.0",
    "babel-loader": "^6.1.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "del": "^2.0.2",
    "h2o2": "^5.1.0",
    "jest-cli": "^0.7.1",
    "object-assign": "^4.0.1",
    "orchestrator": "^0.3.7",
    "react-addons-test-utils": "^0.14.2",
    "webpack": "^1.12.14"
  },
  "dependencies": {
    "babel-register": "^6.7.2",
    "hapi": "^13.2.1",
    "hapi-react-views": "^6.0.0",
    "history": "^1.13.0",
    "immutable": "^3.7.6",
    "inert": "^3.2.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^1.0.3",
    "reflux": "^0.3.0",
    "vision": "^4.0.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/object-assign"
    ]
  }
}
